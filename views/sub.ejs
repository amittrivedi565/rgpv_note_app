<%- include('includes/head') %>

  <body>
    <div class="container-wrap">
      <div class="main-wrapper">
        <div class="main-one">
          <ul style="list-style: none;">
            <div style="font-size: 25px;"><b>RGPV</b>Vault</div>

            <a href="/api/close/dashboard">
              <li><svg xmlns="http://www.w3.org/2000/svg" width="15" height="15" fill="currentColor"
                  class="bi bi-ui-checks-grid" viewBox="0 0 16 16">
                  <path
                    d="M2 10h3a1 1 0 0 1 1 1v3a1 1 0 0 1-1 1H2a1 1 0 0 1-1-1v-3a1 1 0 0 1 1-1m9-9h3a1 1 0 0 1 1 1v3a1 1 0 0 1-1 1h-3a1 1 0 0 1-1-1V2a1 1 0 0 1 1-1m0 9a1 1 0 0 0-1 1v3a1 1 0 0 0 1 1h3a1 1 0 0 0 1-1v-3a1 1 0 0 0-1-1zm0-10a2 2 0 0 0-2 2v3a2 2 0 0 0 2 2h3a2 2 0 0 0 2-2V2a2 2 0 0 0-2-2zM2 9a2 2 0 0 0-2 2v3a2 2 0 0 0 2 2h3a2 2 0 0 0 2-2v-3a2 2 0 0 0-2-2zm7 2a2 2 0 0 1 2-2h3a2 2 0 0 1 2 2v3a2 2 0 0 1-2 2h-3a2 2 0 0 1-2-2zM0 2a2 2 0 0 1 2-2h3a2 2 0 0 1 2 2v3a2 2 0 0 1-2 2H2a2 2 0 0 1-2-2zm5.354.854a.5.5 0 1 0-.708-.708L3 3.793l-.646-.647a.5.5 0 1 0-.708.708l1 1a.5.5 0 0 0 .708 0z" />
                </svg>
                Dashboard
            </a>
            </li>
            <li>
              <a href="/api/close/branch">

                <svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="currentColor"
                  class="bi bi-window-stack" viewBox="0 0 16 16">
                  <path
                    d="M4.5 6a.5.5 0 1 0 0-1 .5.5 0 0 0 0 1M6 6a.5.5 0 1 0 0-1 .5.5 0 0 0 0 1m2-.5a.5.5 0 1 1-1 0 .5.5 0 0 1 1 0" />
                  <path
                    d="M12 1a2 2 0 0 1 2 2 2 2 0 0 1 2 2v8a2 2 0 0 1-2 2H4a2 2 0 0 1-2-2 2 2 0 0 1-2-2V3a2 2 0 0 1 2-2zM2 12V5a2 2 0 0 1 2-2h9a1 1 0 0 0-1-1H2a1 1 0 0 0-1 1v8a1 1 0 0 0 1 1m1-4v5a1 1 0 0 0 1 1h10a1 1 0 0 0 1-1V8zm12-1V5a1 1 0 0 0-1-1H4a1 1 0 0 0-1 1v2z" />
                </svg>
                Manage Panel

              </a>
            </li>

          </ul>
        </div>


        <div class="main-two">
          <div class="bread">
            <h2>Add Subject Notes</h2>
            <div class="bread-content">
              <a href="#">Dashboard</a>
              /
              <a href="/api/close/branch">Branch</a>
              /
              <a href="/api/close/branch">Course</a>
              /
              <a href="/api/close/branch">Year</a>
              /
              <a href="/api/close/branch">Semester</a>

            </div>
          </div>

          <div class="main-section">
            <div class="main-top">

              <div class="btn2">

                <button type="button" class="btn btn-primary btn-sm" data-toggle="modal" data-target="#Modal"
                  data-whatever="@mdo">+</button>

                <div class="modal fade" id="Modal" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel"
                  aria-hidden="true">
                  <div class="modal-dialog" role="document">
                    <div class="modal-content">
                      <div class="modal-header">
                        <h5 class="modal-title" id="exampleModalLabel">Add Course Information
                        </h5>
                        <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                          <span aria-hidden="true">&times;</span>
                        </button>
                      </div>


                      <div class="modal-body">
                        <form method="post">

                          <div class="form-group">
                            <label for="recipient-name" class="col-form-label">Subject
                              Code:</label>
                            <input name="sub_code" type="text" class="form-control" id="recipient-name">

                          </div>
                          <div class="form-group">
                            <label for="recipient-name" class="col-form-label">Subject
                              Name:</label>
                            <input name="sub_name" type="text" class="form-control" id="recipient-name">
                          </div>


                          <div class="form-group">
                            <label for="message-text" class="col-form-label">Subject
                              Description:</label>
                            <textarea name="sub_desc" class="form-control" id="message-text"></textarea>
                          </div>

                          <div class="form-group">
                            <label for="recipient-name" class="col-form-label">Subject
                              Tags:</label>
                            <input name="sub_tags" type="text" class="form-control" id="recipient-name">
                          </div>

                          <div class="form-group">
                            <label for="recipient-name" class="col-form-label">Subject
                              CloudLink:</label>
                            <input name="sub_notes" type="text" class="form-control" id="recipient-name">
                          </div>

                          <div class="modal-footer">
                            <button type="button" class="btn btn-secondary" data-dismiss="modal">Close</button>
                            <button type="submit" class="btn btn-primary">Submit</button>
                          </div>

                        </form>
                      </div>
                    </div>
                  </div>

                </div>

              </div>
            </div>










            <div class="main-bottom">
              <div class="main-accoridan">

                <% if (subData.length>0) { %>
                  <% var index=0; %>
                    <% for( let subArray of subData ) { %>
                      <% index++ %>
                        <div id="accordion">
                          <div style="margin-top: 10px;" class="card">
                            <div class="card-header" id="headingOne">
                              <h5 class="mb-0">
                                <button class="btn btn-link" data-toggle="collapse" data-target="#<%=index%>"
                                  aria-expanded="true" aria-controls="collapseOne">
                                  <b style="color: black;">
                                    <%= subArray.sub_name %>
                                  </b>
                                </button>
                              </h5>
                            </div>
                            <div id="<%= index %>" class="collapse show" aria-labelledby="headingOne"
                              data-parent="#accordion">
                              <div class="card-body">
                                <table class="table align-middle table-sm table-hover">
                                  <thead class="thead-dark">
                                    <tr>
                                     
                        
                                      <th scope="col">Semester</th>
                                      <th scope="col">Code</th>
                                      <th scope="col">Name</th>
                                      <th scope="col">Description</th>
                                      <th scope="col">Tags</th>
                                      <th scope="col">Upload Notes</th>
                                      <th scope="col">Actions</th>
                                    </tr>
                                  </thead>
                                  <tbody>
                                    <tr>
                                    <td><%= subArray.sem_name %></td>
                                      <td>
                                        <%= subArray.sub_code %>
                                      </td>
                                      <td>
                                        <%= subArray.sub_name %>
                                      </td>
                                      <td><a href="#"><button type="button" class="btn btn-link">View</button></a></td>
                                      <td><a href="#"><button type="button" class="btn btn-link">View</button></a></td>
                                 <td><a href="#"><button type="button" class="btn btn-link">Upload</button></a></td>
                                      <form
                                        action="/api/close/branch/course/<%= subArray.sub_id %>?_method=DELETE"
                                        method="post">

                                        <td>
                                          <button type="button" class="btn btn-danger" data-toggle="modal"
                                            data-target="#exampleModal">
                                            Delete
                                          </button>

                                          <!-- Modal -->
                                          <div class="modal fade" id="exampleModal" tabindex="-1" role="dialog"
                                            aria-labelledby="exampleModalLabel" aria-hidden="true">
                                            <div class="modal-dialog" role="document">
                                              <div class="modal-content">
                                                <div class="modal-header">
                                                  <h5 class="modal-title" id="exampleModalLabel">Confirmation For Delete
                                                    Action</h5>
                                                  <button type="button" class="close" data-dismiss="modal"
                                                    aria-label="Close">
                                                    <span aria-hidden="true">&times;</span>
                                                  </button>
                                                </div>
                                                <div class="modal-body">
                                                  *Warning: Deleting This Course Deletes Corresponding Years.
                                                </div>
                                                <div class="modal-footer">
                                                  <button type="button" class="btn btn-secondary"
                                                    data-dismiss="modal">Close</button>
                                                  <button type="submit" class="btn btn-warning">Submit</button>
                                                </div>
                                              </div>
                                            </div>
                                          </div>
                                        </td>

                                      </form>
                                    </tr>
                                  </tbody>
                                </table>
                              </div>
                            </div>
                          </div>

                          <% } %>

                            <% } %>


                        </div>









































              </div>

















            </div>



          </div>
        </div>

      </div>

    </div>

    </div>


    </div>
    </div>
    </div>




  </body>

  </html>